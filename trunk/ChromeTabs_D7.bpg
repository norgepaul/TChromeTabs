ChromeTabs_D7_D.bpl: Packages\Delphi 7\ChromeTabs_D7_D.dpk
  $(DCC)

ChromeTabs_D7_R.bpl: Packages\Delphi 7\ChromeTabs_D7_R.dpk
  $(DCC)

ChromeTabsDemoD7.exe: Demo\ChromeTabsDemoD7.dpr
  $(DCC)


